(use-package awesome-tab
  :load-path "~/.emacs.d/lisp/local"
  :config
  (awesome-tab-mode t)
  (global-set-key (kbd "C-h") 'awesome-tab-forward-tab)
  (global-set-key (kbd "C-l") 'awesome-tab-backward-tab)
  (global-set-key (kbd "C-k") 'awesome-tab-kill-match-buffers-in-current-group)
  )


(provide 'package-awesome-tab)
